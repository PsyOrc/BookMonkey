{
	"name": "ng-cli",
	"displayName": "Angular-CLI commands runner inside VS Code",
	"description": "Angular (v6+) + Angular/Cli commands. Run angular-cli commands inside editor",
	"version": "1.3.0",
	"publisher": "UVBrain",
	"repository": "https://github.com/ultrasonicsoft/ng-cli-vsx",
	"icon": "images/angular-shield.png",
	"engines": {
		"vscode": "^1.25.0"
	},
	"categories": [
		"Extension Packs",
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.ngcli"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.ngcli",
				"title": "ngcli"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"keywords": [
		"Angular",
		"Angular 2",
		"Angular 6",
		"TypeScript",
		"Angular CLI"
	],
	"devDependencies": {
		"typescript": "^2.6.1",
		"vscode": "^1.1.6",
		"tslint": "^5.8.0",
		"@types/node": "^7.0.43",
		"@types/mocha": "^2.2.42"
	},
	"author": "Balram Chavan",
	"__metadata": {
		"id": "17903899-ed7e-4243-a6f1-8fff81e43b14",
		"publisherId": "e2248ff8-ae7a-4029-b435-d1cac8cf64e1",
		"publisherDisplayName": "Balram Chavan"
	}
}